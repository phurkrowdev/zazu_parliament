# Zazu Infrastructure Configuration
# Copy to .env and replace with actual values

# Postgres
POSTGRES_PASSWORD=zazu_dev_password

# MinIO
MINIO_ROOT_USER=zazu_admin
MINIO_ROOT_PASSWORD=zazu_minio_password

# Grafana
GRAFANA_PASSWORD=zazu_grafana_password

# Zazu Core
ZAZU_ENV=development
ZAZU_LOG_LEVEL=INFO
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Constitutional Settings
SENTINEL_HALT_THRESHOLD=0.5  # Anti-paralysis trigger at 50% halt rate
MAX_REPAIR_CYCLES=3
DREAMSPACE_ENABLED=true
